# Production Environment Variables
NODE_ENV=production
PORT=5000

# Database Configuration (ต้องใช้ database ใหม่)
DB_HOST=your_new_database_host
DB_USER=your_new_database_user
DB_PASSWORD=your_new_database_password
DB_NAME=your_new_database_name
DB_PORT=3306

# JWT Secret (ต้องเปลี่ยนใหม่)
JWT_SECRET=your_new_jwt_secret_key_here

# CORS Configuration
CORS_ORIGIN=https://your-frontend-domain.com

# Logging
LOG_LEVEL=info
